{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-developement-1"
		},
		"developementlinkeds_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'developementlinkeds'"
		},
		"testinglinkedservice_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'testinglinkedservice'"
		},
		"developementlinkeds_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storagedevelopement.dfs.core.windows.net/"
		},
		"testinglinkedservice_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storagetestingblob.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF-developement-1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"developementlinkeds_accountKey": {
						"value": "[parameters('developementlinkeds_accountKey')]"
					},
					"testinglinkedservice_accountKey": {
						"value": "[parameters('testinglinkedservice_accountKey')]"
					},
					"developementlinkeds_properties_typeProperties_url": {
						"value": "[parameters('developementlinkeds_properties_typeProperties_url')]"
					},
					"testinglinkedservice_properties_typeProperties_url": {
						"value": "[parameters('testinglinkedservice_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}